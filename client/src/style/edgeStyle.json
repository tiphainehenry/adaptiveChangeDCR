[
    {
      "selector": "edge",
      "style": {
        "width": 5,
        "curve-style": "bezier",
        "source-endpoint": "inside-to-node"
      }
    },
    {
      "selector": "edge.include",
      "style": {
        "line-color": "#29A81A",
        "color":"#29A81A",
        "target-arrow-color": "#29A81A",
        "target-label": "+",
        "fontSize": "20",
        "target-text-offset":"15px",
        "target-text-margin-y":"15px"
      }
    },
    {
      "selector": "edge.exclude",
      "style": {
        "line-color": "#FF0000",
        "target-arrow-color": "#FF0000"
      }
    },
    {
      "selector": "edge.response",
      "style": {
        "line-color": "#1E90FF",
        "target-arrow-color": "#1E90FF",
        "curve-style": "straight"
      }
    },
    {
      "selector": "edge.response.back",
      "style": {
        "line-color": "#1E90FF",
        "target-arrow-color": "#1E90FF",
        "curve-style": "straight",
        "target-arrow-shape": "circle"
      }
    },

    {
      "selector": "edge.condition",
      "style": {
        "line-color": "#FFA500",
        "target-arrow-color": "#FFA500",
        "target-arrow-shape": "circle-triangle"     
      }
    },
    {
      "selector": "edge.milestone",
      "style": {
        "line-color": "#BC1AF2",
        "target-arrow-color": "#BC1AF2",
        "target-arrow-shape": "diamond"
      }
    },
    {
      "selector": "edge:selected",
      "style": {
        "line-color": "#00d1b2",
        "target-arrow-color": "#00d1b2"
      }
    }
  ]