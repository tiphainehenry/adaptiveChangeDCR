				},
				"fetchPublicView(string,address)": {
					"details": "Getter for workflow.",
					"params": {
						"_hash": "index of the workflow (eg 0 for the first workflow).",
						"myAddress": "sender adddress (must be registered to fetch the workflow)."
					},
					"return": "workflow, update didFetch variable to one."
				},
				"requestChange(address,address[],string,string,string)": {
					"details": "manages change requests: if a request is already ongoing, cancel. Else, create a new change request and notify endorsers.",
